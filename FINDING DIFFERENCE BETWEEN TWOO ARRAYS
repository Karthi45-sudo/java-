public class Main {
    public static void main(String[] args) {
        int[] array1 = {4, 5, 2};
        int[] array2 = {1, 7, 4};
        int[] result = new int[array1.length];

        for (int i = 0; i < array1.length; i++) {
            result[i] = array1[i] - array2[i];
        }

        for (int i : result) {
            System.out.print(i + " ");
        }
    }
}
